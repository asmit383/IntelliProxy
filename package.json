{
  "name": "intelliproxy",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "A": "nodemon --watch serverA/serverA.js --exec \"dotenv -e .env.serverA -- node serverA/serverA.js\"",
    "B": "nodemon --watch serverB/serverB.js --exec \"dotenv -e .env.serverB -- node serverB/serverB.js\"",
    "proxy": "nodemon --watch proxy/proxyserver.js --exec \"node proxy/proxyserver.js\"",
    "all": "concurrently \"npm run A\" \"npm run B\" \"npm run proxy\""
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.22.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "http-proxy-middleware": "^3.0.5",
    "nodemon": "^3.1.11",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "dotenv-cli": "^11.0.0"
  }
}
